{
  "lib/*.rb": {
    "type": "lib",
    "alternate": "spec/lib/{}_spec.rb"
  },
  "app/models/*.rb": {
    "type": "model",
    "alternate": "spec/models/{}_spec.rb"
  },
  "bin/*": {
    "type": "bin"
  },
  "app/commands/*.rb": {
    "type": "command",
    "alternate": "spec/commands/{}_spec.rb"
  },
  "db/migrate/*.rb": {
    "type": "migration",
    "alternate": "db/schema.rb"
  },
  "spec/factories/*_factory.rb": {
    "type": "factory",
    "alternate": "app/models/{}.rb"
  },
  "config/application.rb": {
    "type": "root"
  },
  "spec/*_spec.rb": {
    "type": "spec",
    "alternate": "{}.rb"
  },
  "spec/spec_helper.rb": {
    "type": "spec"
  }
}
